# Discord
TOKEN=YOUR_DISCORD_BOT_TOKEN
PREFIX=!!

# Database (optional; if omitted, JSON storage is used)
MONGO_URL=mongodb+srv://username:password@cluster.example.mongodb.net/?retryWrites=true&w=majority

# Slash command deployment
# Comma-separated guild IDs for fast per-guild registration (e.g. 111111111111111111,222222222222222222)
GUILD_ID=
# Set to true to register globally (can take up to 1 hour to propagate)
SLASH_GLOBAL=false

# Express server
PORT=3000

# Suppress noisy update checks from ytsr/ytdl
YTSR_NO_UPDATE=true
YTDL_NO_UPDATE=true

# Voice diagnostics (optional). Set to true to print a dependency report on startup
VOICE_DEBUG_REPORT=false

# Optional: path to ffmpeg (auto-detected if not set)
# FFMPEG_PATH=C:\\path\\to\\ffmpeg.exe

# High-quality mode: avoid transforms (filters/volume) for fastest Opus passthrough
HQ_MODE=true

# Optional: YouTube account cookie to reduce throttling/age gating
# YOUTUBE_COOKIE=VISITOR_INFO1_LIVE=...; PREF=...; SAPISID=...; HSID=...;
